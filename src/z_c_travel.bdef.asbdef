projection;
strict;
use draft;

define behavior for Z_C_TRAVEL //alias <alias_name>
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Prepare;
  use action Resume;


  use action cancelTravel;
  use function countTravel;

  use association _booking {create; with draft;}
}

define behavior for Z_C_Booking alias Booking {
  use update;
  use delete;

  use association _travel {with draft;}
}